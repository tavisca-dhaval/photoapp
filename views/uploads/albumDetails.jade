extends ../layout.jade

block content
	div.container-fluid 
		<h2 id="albumName" album-name="#{albumName}">##{albumName}</h2>
		ul.cat-images
			each imPath in categoryImage
				li
					div.img-box
						- if(user && user.admin){
							span.glyphicon.glyphicon-remove-circle.remove-from-album(id="#{imPath._id}")
						- }
						img(src="/images/thumb/#{imPath.filename}")